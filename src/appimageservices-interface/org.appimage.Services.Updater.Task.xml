<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.appimage.Services1.Updater.Task">
    <property name="id" type="s" access="read"/>
    <property name="state" type="i" access="read"/>
    <property name="progressValue" type="i" access="read"/>
    <property name="progressTotal" type="i" access="read"/>
    <property name="statusMessage" type="s" access="read"/>
    <signal name="stateChanged">
      <arg name="state" type="i" direction="out"/>
    </signal>
    <signal name="statusMessageChanged">
      <arg name="state" type="s" direction="out"/>
    </signal>
    <signal name="progressValueChanged">
      <arg name="value" type="i" direction="out"/>
    </signal>
    <signal name="progressTotalChanged">
      <arg name="value" type="i" direction="out"/>
      <arg name="total" type="i" direction="out"/>
    </signal>
    <method name="cancel">
    </method>
  </interface>
</node>
